<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="@dimen/imageView.inset"
    android:insetRight="@dimen/imageView.inset"
    android:insetBottom="@dimen/imageView.inset"
    android:insetTop="@dimen/imageView.inset">

    <shape>

        <solid android:color="@color/white_t" />
        <gradient
            android:endColor="@color/white_t"
            android:gradientRadius="@android:color/white"
            android:type="radial" />

        <corners android:radius="@dimen/imageView.radius"/>

    </shape>


</inset>